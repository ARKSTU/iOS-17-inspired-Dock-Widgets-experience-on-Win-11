# EditorConfig for iOS 17 Launcher
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{kt,kts}]
indent_size = 4
continuation_indent_size = 4

[*.{xml,json,yml,yaml}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.{gradle,properties}]
indent_size = 4
